<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0-windows7.0</TargetFramework>
    <UseWPF>true</UseWPF>
    <Version>0.2.0.0</Version>
    <PackageId>TwineStreamerBotLib</PackageId>
    <AssemblyName>TwineStreamerBotLib</AssemblyName>
    <Authors>WrithemTwine</Authors>
    <Company>TwineStreamerBotLib</Company>
    <Product>TwineStreamerBotLib</Product>
    <EnforceCodeStyleInBuild>True</EnforceCodeStyleInBuild>
    <GenerateDocumentationFile>False</GenerateDocumentationFile>
    <UseWindowsForms>True</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="6.0.0" />
    <PackageReference Include="TwitchLib.Api" Version="3.5.2" />
    <PackageReference Include="TwitchLib.Api.Core" Version="3.5.2" />
    <PackageReference Include="TwitchLib.Api.Helix" Version="3.5.2" />
    <PackageReference Include="TwitchLib.Client" Version="3.2.6-preview-6109ec73405e067631aac6c8c837bc87dec6ad63" />
    <PackageReference Include="TwitchLib.PubSub" Version="3.2.5-preview-e27b78571148879a04eaea2cf45441a35b0e908f" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\MultiUserLiveBot\MultiUserLiveBot.csproj" />
    <ProjectReference Include="..\MediaOverlayServer\MediaOverlayServer.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="BotClients\Twitch\TwitchLib\Core\ServiceTimer.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Culture\Msgs.Designer.cs">
      <DependentUpon>Msgs.resx</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
    <Compile Update="Data\DataSource.Designer.cs">
      <DependentUpon>DataSource.xsd</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <DependentUpon>Resources.resx</DependentUpon>
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
    </Compile>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <DependentUpon>Settings.settings</DependentUpon>
      <AutoGen>True</AutoGen>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Culture\Msgs.resx">
      <LastGenOutput>Msgs.Designer.cs</LastGenOutput>
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
      <Generator>ResXFileCodeGenerator</Generator>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Data\DataSource.xsd">
      <LastGenOutput>DataSource.Designer.cs</LastGenOutput>
      <Generator>MSDataSetGenerator</Generator>
    </None>
    <None Update="Properties\Settings.settings">
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
      <Generator>PublicSettingsSingleFileGenerator</Generator>
    </None>
  </ItemGroup>
  


</Project>

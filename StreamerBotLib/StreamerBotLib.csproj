<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0-windows</TargetFramework>
		<Nullable>disable</Nullable>
		<UseWPF>true</UseWPF>
		<ImplicitUsings>enable</ImplicitUsings>
		<Version>1.3.1.1</Version>
		<PackageId>StreamerBotLib</PackageId>
		<AssemblyName>StreamerBotLib</AssemblyName>
		<Authors>WrithemTwine</Authors>
		<Company>TwineStreamerBotLib</Company>
		<Product>TwineStreamerBotLib</Product>
		<EnforceCodeStyleInBuild>True</EnforceCodeStyleInBuild>
		<GenerateDocumentationFile>False</GenerateDocumentationFile>
		<Copyright>2024</Copyright>
		<Configurations>Debug;Release;Release_Sqlite;Release_PostGre;Release_KNet;Release_Cosmos;Release_MySql;Release_SqlServer</Configurations>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
	  <NoWarn>1701;1702;SYSLIB0051</NoWarn>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
	  <NoWarn>1701;1702;SYSLIB0051</NoWarn>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release_SqlServer|AnyCPU'">
	  <NoWarn>1701;1702;SYSLIB0051</NoWarn>
		<Optimize>True</Optimize>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release_Cosmos|AnyCPU'">
	  <NoWarn>1701;1702;SYSLIB0051</NoWarn>
		<Optimize>True</Optimize>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release_KNet|AnyCPU'">
	  <NoWarn>1701;1702;SYSLIB0051</NoWarn>
		<Optimize>True</Optimize>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release_PostGre|AnyCPU'">
	  <NoWarn>1701;1702;SYSLIB0051</NoWarn>
	  <Optimize>True</Optimize>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release_Sqlite|AnyCPU'">
	  <NoWarn>1701;1702;SYSLIB0051</NoWarn>
	  <Optimize>True</Optimize>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release_MySql|AnyCPU'">
		<NoWarn>1701;1702;SYSLIB0051</NoWarn>
		<Optimize>True</Optimize>
	</PropertyGroup>
	
	<ItemGroup>
		<Folder Include="MultiLive\Culture\" />
	</ItemGroup>

	<ItemGroup>
		<None Include="Properties\PublishProfiles\ClickOnceProfile.pubxml.user" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="MASES.EntityFrameworkCore.KNet" Version="2.3.0" Condition="'$(Configuration)|$(Platform)'=='Release_KNet|AnyCPU'" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.Cosmos" Version="8.0.6" Condition="'$(Configuration)|$(Platform)'=='Release_Cosmos|AnyCPU'" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="8.0.6" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="8.0.6" Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="8.0.6" Condition="'$(Configuration)|$(Platform)'=='Release_Sqlite|AnyCPU'" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="8.0.6" Condition="'$(Configuration)|$(Platform)'=='Release_SqlServer|AnyCPU'" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.Tools" Version="8.0.6">
		  <PrivateAssets>all</PrivateAssets>
		  <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
		</PackageReference>
		<PackageReference Include="Microsoft.Extensions.Logging" Version="8.0.0" />
		<PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="8.0.1" />
		<PackageReference Include="Microsoft.Extensions.Logging.Console" Version="8.0.0" />
		<PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL" Version="8.0.4" Condition="'$(Configuration)|$(Platform)'=='Release_PostGre|AnyCPU'" />
		<PackageReference Include="Pomelo.EntityFrameworkCore.MySql" Version="8.0.2" Condition="'$(Configuration)|$(Platform)'=='Release_MySql|AnyCPU'" />
		<PackageReference Include="System.Drawing.Common" Version="8.0.6" />
		<PackageReference Include="TwitchLib.Api" Version="3.9.0" />
		<PackageReference Include="TwitchLib.Api.Core" Version="3.9.0" />
		<PackageReference Include="TwitchLib.Api.Helix" Version="3.9.0" />
		<PackageReference Include="TwitchLib.Client" Version="3.4.0-preview-d6687358268723051c755034b155a703c97c353a" />
		<PackageReference Include="TwitchLib.PubSub" Version="3.2.6" />
	</ItemGroup>

	<ItemGroup>
	  <Resource Include="Overlay\img\seconds.png" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="DataSQL\Import\DataSource.Designer.cs">
		  <DesignTime>True</DesignTime>
		  <AutoGen>True</AutoGen>
		  <DependentUpon>DataSource.xsd</DependentUpon>
		</Compile>
		<Compile Update="DataSQL\Import\Multi\MultiDataSource.Designer.cs">
		  <DesignTime>True</DesignTime>
		  <AutoGen>True</AutoGen>
		  <DependentUpon>MultiDataSource.xsd</DependentUpon>
		</Compile>
		<Compile Update="Properties\Resources.Designer.cs">
		  <DesignTime>True</DesignTime>
		  <AutoGen>True</AutoGen>
		  <DependentUpon>Resources.resx</DependentUpon>
		</Compile>
		<Compile Update="Properties\Settings.Designer.cs">
		  <DesignTimeSharedInput>True</DesignTimeSharedInput>
		  <AutoGen>True</AutoGen>
		  <DependentUpon>Settings.settings</DependentUpon>
		</Compile>
	</ItemGroup>

	<ItemGroup>
	  <EmbeddedResource Update="Properties\Resources.resx">
	    <Generator>PublicResXFileCodeGenerator</Generator>
	    <LastGenOutput>Resources.Designer.cs</LastGenOutput>
	  </EmbeddedResource>
	</ItemGroup>

	<ItemGroup>
		<None Update="DataSQL\Import\DataSource.xsd">
		  <Generator>MSDataSetGenerator</Generator>
		  <LastGenOutput>DataSource.Designer.cs</LastGenOutput>
		</None>
		<None Update="DataSQL\Import\Multi\MultiDataSource.xsd">
		  <Generator>MSDataSetGenerator</Generator>
		  <LastGenOutput>MultiDataSource.Designer.cs</LastGenOutput>
		</None>
		<None Update="Properties\Settings.settings">
		  <Generator>PublicSettingsSingleFileGenerator</Generator>
		  <LastGenOutput>Settings.Designer.cs</LastGenOutput>
		</None>
	</ItemGroup>

</Project>

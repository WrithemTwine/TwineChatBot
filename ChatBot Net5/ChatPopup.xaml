<Page x:Class="ChatBot_Net5.ChatPopup"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ChatBot_Net5"
      mc:Ignorable="d" 
      MinHeight="450" MinWidth="230"
      Title="ChatPopup" SnapsToDevicePixels="True" Unloaded="Page_Unloaded">

    <DockPanel>
        <RichTextBox x:Name="Page_ChatPopup_RichText" SnapsToDevicePixels="True" UseLayoutRounding="True" Opacity="50" />
    </DockPanel>
</Page>
